                }
            ],
            "transform": [
                {
                    "type": "stratify",
                    "key": "id",
                    "parentKey": "parent"
                },
                {
                    "type": "tree",
                    "method": {
                        "signal": "layout"
                    },
                    "size": [
                        1,
                        1
                    ],
                    "as": [
                        "alpha",
                        "beta",
                        "depth",
                        "children"
                    ]
                },
                {
                    "type": "formula",
                    "expr": "(rotate + extent * datum.alpha + 270) % 360",
                    "as": "angle"
                },
                {
                    "type": "formula",
                    "expr": "inrange(datum.angle, [90, 270])",
                    "as": "leftside"
                },
                {
                    "type": "formula",
                    "expr": "originX + radius * datum.beta * cos(PI * datum.angle / 180)",
                    "as": "x"
                },
                {
                    "type": "formula",
                    "expr": "originY + radius * datum.beta * sin(PI * datum.angle / 180)",
                    "as": "y"
                }
            ]
        },
