        },
        {
            "name": "colorIn",
            "value": "firebrick"
        },
        {
            "name": "colorOut",
            "value": "forestgreen"
        },
        {
            "name": "originX",
            "update": "width / 2"
        },
        {
            "name": "originY",
            "update": "height / 2"
        },
        {
            "name": "active",
            "value": null,
            "on": [
                {
                    "events": "text:mouseover",
                    "update": "datum.id"
                },
                {
                    "events": "mouseover[!event.item]",
                    "update": "null"
                }
            ]
        }
    ],
